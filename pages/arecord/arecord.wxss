.top{
  
  background-image: -webkit-linear-gradient(top,white,#C3DDFA);
}

.box {
  display: flex;
  justify-content: center;
}

.pic {
  display: flex;
  justify-content: center;
  margin-top: 20rpx;
}

.pic image{
  -webkit-box-shadow:  0px 6px 10px 10px rgba(245, 241, 241, 0.5);
  -moz-box-shadow:  0px 6px 10px 10px rgba(247, 243, 243, 0.5);
  box-shadow: 0px 6px 10px 10px rgba(250, 249, 249, 0.5);   
}

.left {
  width: 30%;
  margin-top: 80rpx;
  margin-left: 100rpx;
  float: left;
  color: #13227a;
  background-color: rgba(255, 255, 255, 0.623);
}

.right {
  width: 30%;
  margin-top: 80rpx;
  margin-right: 100rpx;
  float: right;
  color: #13227a;
  background-color: rgba(255, 255, 255, 0.623)
}

.yuan {
  display: flex;
  width: 30%;
  border-radius: 5%;
  margin-top: 100rpx;
  margin-bottom: 50rpx;
  background-color: rgba(255, 255, 255, 0.623);
  background-size: 40rpx 40rpx;
  align-items: center;
  flex-direction: center;
  justify-content: center;
  color: #13227a;
}

.hover {
  background-color: rgb(126, 184, 243);
}


.table {
  border: 0px solid darkgray;
  font-size: 12px;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
 }
 .td {
   width:40%;
    justify-content: center; 
   text-align: center;
   margin:0px;
      padding:0px;
      background-color:#c2ebf5;
      font-family:"微软雅黑","黑体","宋体";
      font-size:12px;
      height:36px;

 }
 .bg-w{
  background: snow;
 }
   
 .th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 2rem;
  align-items: center;
 } 

 .page-wrapper {
  margin: 0;
  font-size: 28rpx;
  line-height: 1.5;
  color: #333;
  background-color: #efefef;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
  -webkit-tap-highlight-color: transparent;
  min-height:2000rpx;
}